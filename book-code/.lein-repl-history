exit
(defrecord Store [name inventory])
(def lider (->Store "Lider" #{:bacon 3 :milk 4 :meat 2})
)
(:bacon lider)
(:inventory lider)
(keys lider)
(:inventory lider)
(:bacon (:inventory lider))
(get :bacon (:inventory lider))
(get (:inventory lider) :bacon)
((:inventory lider) :bacon)
(def lider (->Store "Lider" {:bacon 3 :milk 4 :meat 2}))
(:bacon (:inventory lider))
(:inventory lider)
(doc get-in)
(get-in lider [inventory bacon]
)
(get-in lider [inventory bacon])
(get-in lider [bacon])
(get-in lider [:bacon])
(get-in lider [inventory :bacon])
